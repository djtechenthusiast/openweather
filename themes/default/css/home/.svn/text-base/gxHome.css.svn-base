html,
body {
	margin:0;
	padding:0;
	height:100%;
	padding-top: 0px;
	color: #5B5B5B;
	font-family: 'Open Sans', Tahoma, Arial, Helvetica, sans-serif;
}
#wrapper {
    min-height: 92%;
    height: 100%;
}
#header {
	
}

#content {
   height:auto;
   min-height: 77%;
   position:relative;
}
#footer {
   clear: both;
   bottom:0;
   width:100%;
   height:55px;   /* Height of the footer */
}


#btnPopover {
	cursor: pointer;
}

.navbar-brand {
	float: left;
	height: 80px;
	padding: 0px 15px;
	line-height: 20px;
}

.navbar-nav {
	float: left;
	margin: 0;
	padding-top: 25px;
}

.headicon-size {
	font-size: 21px;
}

.navbar {
position: relative;
min-height: 50px;
margin-bottom: 0px;
border: 1px solid transparent;
}

.navbar-nav>li {
	float: left;
	margin-left: 15px;
}

.navbar-default {
	background-color: #FFFFFF;
	border-color: transparent;
}

.navbar-nav>li>a {
	padding-top: 11px;
	padding-bottom: 11px;
}

.nav>li>a {
	position: relative;
	display: block;
	padding: 9px 9px;
	height: 42px;
}

.navbar-default .navbar-nav>li>a {
	color: #FFF;
	background-color: #00A8E7;
	border-radius: 4px;
	cursor: pointer;
}

.navbar-default .navbar-nav>.active>a,.navbar-default .navbar-nav>.active>a:hover,.navbar-default .navbar-nav>.active>a:focus
	{
	color: #FFF;
	background-color: #93268f;
}

.navbar-default .navbar-nav>li>a:hover,.navbar-default .navbar-nav>li>a:focus
	{
	color: #FFF;
	background-color: #93268f;
}

.navbar-nav.navbar-right:last-child {
	margin-right: -15px;
}

.alertsbadge {
	float: right;
	text-align: left;
	margin: 1px 0px 0 5px;
	padding: 5px;
	border-radius: 4px;
	display: inline-block;
	min-width: 10px;
	font-size: 11px;
	font-weight: 600;
	line-height: 1;
	color: #fff;
	white-space: nowrap;
	vertical-align: baseline;
	background-color: #FF0000;
}



.glyphicon-user {
	height: 21px;
}

.navbar-usercontainer {
	display: block;
	margin-top: -34px;
	padding-left: 25px;
}

.navbar-username {
	font-size: 14px;
	display: inline-block;
	margin: 2px 0px -5px 0px;
	padding: 2px 0px 0px 5px;
}

.navbar-userrole {
	font-size: 12px;
	padding-left: 5px;
	margin: 0px 0px -5px 0px;
}

.breadcrumb {
	padding: 8px 3px;
	margin-bottom: 0px;
	list-style: none;
	background-color: transparent;
	border-radius: 4px;
	font-size: 11px;
	color: #66429A;
}

ol a {
	color: #00A8E8;
}

ol a:hover,a:focus {
	color: #FF0000;
}

footer {
height: 43px;
width: 98.1%;
margin: 5px 5px 5px 15px;
overflow: hidden;
font-size: 11px;
color: #727272;
background-color: #ebebeb;
border-radius: 4px;
}

footer a {
	color: #00A8E8;
}

footer a:hover,a:focus {
	color: #FF0000;
}

footer .row {
	margin-right: -15px;
	margin-left: -15px;
	margin-top: 15px
}

.popover-content {
	padding: 10px 10px;
}

a:focus,a:active:focus,a.active:focus {
	outline: none;
}

/* BEGIN: Icon Classes */
.gxIcon-home {
	background-image: url("../../images/icons/white/home.png");
	width: 24px;
	height: 24px;
	display: block;
}

.gxIcon-menu {
	background-image: url("../../images/icons/white/menu.png");
	width: 24px;
	height: 24px;
	display: block;
}

.gxIcon-favorites {
	background-image: url("../../images/icons/white/favorites.png");
	width: 24px;
	height: 24px;
	display: block;
}

.gxIcon-alert {
	background-image: url("../../images/icons/white/alert.png");
	width: 24px;
	height: 24px;
	display: inline-block;
}

.gxIcon-dashboard {
	background-image: url("../../images/icons/white/dashboard.png");
	width: 24px;
	height: 24px;
	display: block;
}

.gxIcon-help-global {
	background-image: url("../../images/icons/white/help-global.png");
	width: 24px;
	height: 24px;
	display: block;
}

.gxIcon-user-profile {
	background-image: url("../../images/icons/white/user-profile.png");
	width: 24px;
	height: 24px;
	display: inline-block;
}

.gxIcon-logout {
	background-image: url("../../images/icons/white/logout.png");
	width: 24px;
	height: 24px;
	display: block;
}

/* END: Icon Classes */

/*BEGIN: Menu Custom Styles  */
.panel-body a {
	color: #00A8E7;
	text-decoration: none;
}

.panel-body a:hover,a:focus {
	color: #FF0000;
	text-decoration: none;
}

.panel-default>.panel-heading {
	background-color: #00A8E7;
	color: #FFF;
	border-color: #ddd;
}

.panel-default>.panel-heading:hover {
	background-color: #93268f;
	color: #FFF;
	border-color: #ddd;
}

.panel-default>.panel-heading:active {
	background-color: #93268f;
	color: #FFF;
	border-color: #ddd;
}

.panel-active {
	background-color: #93268f;
	color: #FFF;
	border-color: #ddd;
}

.panel-title a {
	margin-top: 0;
	margin-bottom: 0;
	font-size: 14px;
	color: inherit;
	text-decoration: none;
	cursor: pointer;
}

.panel-title a:hover,a:focus {
	text-decoration: none;
	cursor: pointer;
}

.glyphicon-chevron-right {
	font-size: 12px;
}

.glyphicon-chevron-down {
	font-size: 12px;
}

.ng-binding span {
	font-size: 12px;
}

/* .popover {
	top: 50px !important;
	left: -110px !important;
	display: block;
} */

.panel-group {
	margin-bottom: 5px;
	min-width: 244px;
}

.navbar-nav>li>a:active {
	color: #FFF;
	background-color: #A152A0;
}

.panel-group .panel+.panel {
	margin-top: 0px;
}

/* END: Menu Custom Styles  */